{
  "rules": 
    "projects": {
      ".read": true,
      "$projects": {
        "votes": {
          ".write": "auth !== null"
        },
        "partners": {
          ".write": "auth !== null"
        },
        "$posts": {
          ".write": "root.child('projects/' + $projects + '/uid').val() === auth.uid"
        }
      }  
    }
  }
}
